# ****************************************************************
#
# (note)
# Important Flags
# F77FORTRAN : Create libraries using f77 compiler
# F90FORTRAN : Create libraries using f90 compiler
# DOSHRLIB   : Create shared libraries
#   default setting is define F90FORTRAN and DOSHRLIB
# ****************************************************************
# TO change configuration , edit following three lines properly.
# #define F77FORTRAN

SHRLIBFLAGS = +z
SHLIBLDFLAGS = -b -E
INSTLIBFLAGS = -m 555

#  ************************
#  F77FORTRAN
#  ************************

#  ************************
#
#  ************************

FC = f90
F90OPTS=-L/usr/lib -i,E,U,P,PL,LT,L,EU,CL,CU,BL -W0,"LANGLVL(CONTI199,DARG(L),H8000)"
FFLAGS = $(CPPOPT) $(INCPATH) $(F90OPTS)

.F.o:
	$(FC) ./$*.F -c $(F90OPTS) $(CPPOPT) $(INCPATH) $(SHRLIBFLAGS) -o $*.o
.f.o:
	$(FC) -c $(F90OPTS) $(SHRLIBFLAGS) -O  $<

#  ************************
#  Setting common to F90 and F77
#  ************************
FF      = $(FC)
CC	= cc
CPP     = /lib/cpp
CPPOPT  = -DHIUXF77 -DKEK -DUNIX -DJLC $(EXTRADEFINES)
CFLAGS  = -O -DHIUXF77 -DKEK -DUNIX -DJLC $(EXTRADEFINES) $(SHRLIBFLAGS) $(INCPATH)
RANLIB  = /bin/ranlib
INSTALL = install

# ****************************************************************
# MachTenPPCArchitecture
# ****************************************************************

# ****************************************************************
# PPCArchitecture
# ****************************************************************

# ****************************************************************
# LinuxArchitecture
# ****************************************************************

LCDIR  = ../../..
