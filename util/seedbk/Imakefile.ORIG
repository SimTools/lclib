#include "../../config/lclib.tmpl"
INSTALLDIR = ../../lib

LOCAL_LIB   =  libSEEDBK.a 
OBJS = dsmcsd.o \
		mcsdpr.o \
		mcsdri.o \
		mcsdrt.o \
		mcseed.o
LD = $(F77)
all::	$(LOCAL_LIB) 


$(LOCAL_LIB):: $(OBJS)
	$(AR) rv  $(LOCAL_LIB) $(OBJS)
	$(RANLIB) $(LOCAL_LIB)

install:: $(LOCAL_LIB)
	rm -f $(INSTALLDIR)/$(LOCAL_LIB)
	cp -p $(LOCAL_LIB) $(INSTALLDIR)/$(LOCAL_LIB)

clean::
	rm -f *.o; rm -f *.a; rm -f *~ 

###
dsmcsd.o: FLDSCR.inc FLLEVL.inc PRJFLG.inc
mcsdpr.o: MCSDFG.inc
mcsdri.o: MCSDFG.inc
mcsdrt.o: MCSDFG.inc
mcseed.o: MCSDFG.inc
