COMMANDS = allconv chname f2unix

all:: chname f2unix

chname: chname.o
	$(CC) -o chname chname.c

f2unix: f2unix.o
	$(CC) -o f2unix f2unix.c

install::
	@for i in $(COMMANDS); do  	    cp -p $$i ../../../bin;  	done

clean::
	rm -f chname f2unix *.o *~ *.bak

