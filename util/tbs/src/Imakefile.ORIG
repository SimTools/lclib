#ifdef HPArchitecture
OBJS	      = $(F_OBJS) $(C_OBJS) $(HP_OBJS) $(UNIX_OBJS)
#endif
#ifdef hitachiArchitecture
OBJS	      = $(F_OBJS) $(C_OBJS) $(HP_OBJS) $(UNIX_OBJS) FNUM.o
#endif


TBSLIB = libTBS.a
LIBDIR = ../../../lib

############################################

all:	$(TBSLIB)

$(TBSLIB):     $(OBJS)
	ar r $(TBSLIB) $(OBJS)
	ranlib $(TBSLIB)

clean::
	rm -f $(OBJS) $(TBSLIB) *~

install:: $(TBSLIB)
	rm -f $(LIBDIR)/$(TBSLIB)
	cp -p $(TBSLIB) $(LIBDIR)/$(TBSLIB)
	ranlib $(LIBDIR)/$(TBSLIB)

